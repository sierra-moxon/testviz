@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix testviz: <https://w3id.org/sierra-moxon/testviz/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

testviz:PersonCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "PersonCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom testviz:Person ;
            owl:onProperty testviz:entries ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty testviz:entries ] ;
    skos:definition "A holder for Person objects" ;
    skos:inScheme <https://w3id.org/sierra-moxon/testviz> .

testviz:age_in_years a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "age_in_years" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of years since birth" ;
    skos:inScheme <https://w3id.org/sierra-moxon/testviz> .

testviz:birth_date a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "birth_date" ;
    rdfs:range linkml:Date ;
    skos:definition "Date on which a person is born" ;
    skos:inScheme <https://w3id.org/sierra-moxon/testviz> .

testviz:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    skos:definition "A human-readable description for a thing" ;
    skos:inScheme <https://w3id.org/sierra-moxon/testviz> .

testviz:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:inScheme <https://w3id.org/sierra-moxon/testviz> .

testviz:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    skos:definition "A human-readable name for a thing" ;
    skos:inScheme <https://w3id.org/sierra-moxon/testviz> .

testviz:vital_status a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "vital_status" ;
    rdfs:range testviz:PersonStatus ;
    skos:definition "living or dead status" ;
    skos:inScheme <https://w3id.org/sierra-moxon/testviz> .

testviz:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema1:Thing ;
    skos:inScheme <https://w3id.org/sierra-moxon/testviz> .

testviz:Person a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Person" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:onDatatype xsd:string ;
                    owl:withRestrictions ( [ xsd:pattern "^\\S+@[\\S+\\.]+\\S+" ] ) ] ;
            owl:onProperty testviz:primary_email ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty testviz:primary_email ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty testviz:primary_email ],
        testviz:NamedThing ;
    skos:definition "Represents a Person" ;
    skos:inScheme <https://w3id.org/sierra-moxon/testviz> .

PATO:0001421 a owl:Class,
        testviz:PersonStatus ;
    rdfs:label "ALIVE" .

PATO:0001422 a owl:Class,
        testviz:PersonStatus ;
    rdfs:label "DEAD" .

<https://w3id.org/sierra-moxon/testviz/PersonStatus#UNKNOWN> a owl:Class,
        testviz:PersonStatus ;
    rdfs:label "UNKNOWN" .

testviz:entries a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "entries" ;
    skos:inScheme <https://w3id.org/sierra-moxon/testviz> .

testviz:primary_email a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "primary_email" ;
    skos:definition "The main email address of a person" ;
    skos:inScheme <https://w3id.org/sierra-moxon/testviz> .

testviz:PersonStatus a owl:Class,
        linkml:EnumDefinition ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/sierra-moxon/testviz/PersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/sierra-moxon/testviz/PersonStatus#UNKNOWN> .

<https://w3id.org/sierra-moxon/testviz> a owl:Ontology ;
    rdfs:label "testviz" ;
    dcterms:license "MIT" ;
    dcterms:title "testviz" ;
    rdfs:seeAlso <https://sierra-moxon.github.io/testviz> ;
    skos:definition "This is the project description." .

